<h1>Show the user detail</h1>
<%= link_to("Back to users index page", users_path)%>
<table summary="User List">
  <tr>
    <th>#</th>
    <th>Username</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Gender</th>
    <th>Date of Birth</th>
    <th>Phone Number</th>
    <th>Address</th>
  </tr>
  <tr>
    <td><%= @user.id%></td>
    <td><%= @user.username%></td>
    <td><%= @user.first_name%></td>
    <td><%= @user.last_name%></td>
    <td><%= @gender[@user.gender]%></td>
    <td><%= @user.date_of_birth%></td>
    <td><%= @user.phone_number%></td>
    <td><%= @user.address%></td>
  </tr>
</table>
