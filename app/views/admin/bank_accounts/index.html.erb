<%= link_to("Back to dashboard", "/admin/dashboard")%> <br/>
<h1> Bank account Management</h1>
<%= link_to("Create a new bank account", {:action => "new"})%>
<table summary=" Bank accounts List">
  <tr>
    <th>Account Id</th>
    <th>Currency </th>
    <th>Amount </th>
  </tr>
  <% @bankAccounts.each do |bankAccount|%>
  <tr>
    <td><%= bankAccount.account_id%></td>
    <td><%= bankAccount.currency%></td>
    <td><%= bankAccount.amount%></td>
    <td><%= link_to("Show", admin_bank_account_path(bankAccount)) %></td>
    <td><%= link_to("Edit", edit_admin_bank_account_path(bankAccount))%></td>
    <td><%= link_to("Delete", delete_admin_bank_account_path(bankAccount)) %></td>
  </tr>
  <% end %>
</table>
